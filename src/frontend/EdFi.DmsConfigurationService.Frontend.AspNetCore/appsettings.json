{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "IdentitySettings": {
        "AuthenticationScheme": "oidc",
        "Authority": "http://localhost:8045/realms/dms",
        "ClientId": "DmsConfigurationService",
        "ClientSecret": "",
        "ResponseType": "code id_token",
        "RequireHttpsMetadata": false,
        "SaveTokens": true,
        "GetClaimsFromUserInfoEndpoint": true,
        "Scopes": [ "openid", "email" ],
        "ClaimTypeMappings": {
            "NameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
            "IdentifierClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/nameidentifier",
            "EmailClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/emailaddress",
            "RoleClaimType": "http://schemas.microsoft.com/ws/2008/06/identity/claims/role"
        }
    },
    "AllowedHosts": "*"
}
